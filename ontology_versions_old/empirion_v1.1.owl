        <rdfs:comment>Number of the trial.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3id.org/empirion#ExperimentSettings -->

    <owl:Class rdf:about="http://www.w3id.org/empirion#ExperimentSettings">
